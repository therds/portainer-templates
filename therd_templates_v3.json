{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "name": "freshrss",
      "title": "FreshRSS",
      "description": "A free, self-hostable rss aggregator. (image:linuxserver.io)",
      "type": 1,
      "categories": [
        "Smarthome"
      ],
      "platform": "linux",
      "image": "linuxserver/freshrss:latest",
      "logo": "https://raw.githubusercontent.com/therds/portainer-templates/refs/heads/main/stack/freshrss-logo.png",
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "1000",
          "label": "PGID",
          "name": "PGID"
        }
      ],
      "ports": [
        "80/tcp"
      ],
      "restart_policy": "unless-stopped",
      "volumes": [
        {
          "bind": "/portainer/Files/AppData/Config/freshrss",
          "container": "/config"
        }
      ],
      "maintainer": "https://github.com/therds/portainer-templates"
    },
    {
      "id": 2,
      "name": "freshrss-stack",
      "title": "FreshRSS-stack",
      "description": "A free, self-hostable rss aggregator. (image:linuxserver.io)",
      "logo": "https://raw.githubusercontent.com/therds/portainer-templates/refs/heads/main/stack/freshrss-logo.png",
      "image": "linuxserver/freshrss:latest",
      "platform": "linux",
      "type": 3,
      "categories": [
        "Smarthome"
      ],
      "env": [
        {
          "default": "1000",
          "label": "PUID",
          "name": "PUID"
        },
        {
          "default": "1000",
          "label": "PGID",
          "name": "PGID"
        },
        {
          "default": "freshrss",
          "label": "HOSTNAME",
          "name": "HOSTNAME"
        },
        {
          "default": "4080",
          "label": "PORT",
          "name": "PORT"
        },
        {
          "default": "/share/portainer/freshrss", 
          "label": "DATA",
          "name": "DATA"
        },
        {
          "default": "Asia/Bangkok", 
          "label": "TZ",
          "name": "TZ"
        }
      ],
      "repository": {
        "stackfile": "stack/freshrss-compose.yml",
        "url": "https://github.com/therds/portainer-templates"
      },
      "maintainer": "https://github.com/therds/portainer-templates"
    }

  ]
}
